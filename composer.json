{
    "name": "cosmow/rabbitmq-elastica-bundle",
    "description": "Cosmomedia's fork of Vadiktok's RabbitMQ Elastica Bundle.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Milayton Pereira",
            "email": "milayton.pereira@cosmomedia.es"
        }
    ],
    "require": {
        "php": "^8.0 || ^7.4 || ^7.3",
        "friendsofsymfony/elastica-bundle": "^6.3",
        "php-amqplib/rabbitmq-bundle": "^2.16.0"
    },
    "autoload": {
        "psr-4": {
            "Cosmow\\RabbitMQElasticaBundle\\": "src/RabbitMQElasticaBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cosmow\\RabbitMQElasticaBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
